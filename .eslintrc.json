{
    "env": {
        "es2021": true,
        "webextensions": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "arrow-parens": "off",
      "import/extensions": { "js": "always" }
    }
}
