{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "globals": {
      "chrome": true,
      "fetch": true,
      "URL": true,
      "WebSocket": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      }
    }
}
